{
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.1"
  },
  "tasks": {
    "build": "deno run -A build.ts",
    "test": "deno test --allow-env --allow-net test/*.test.ts",
    "test:integration": "deno test --allow-env --allow-net test/integration/",
    "test:all": "deno test --allow-env --allow-net test/",
    "test:coverage": "deno test --allow-env --allow-net --coverage=coverage test/ && deno coverage coverage"
  }
}
